
colorbody= #FEFDFD
colorheader=#E5E5E5
colorfooter=#3BBAB5
colortitulos=#8C8B8B
negro=#383838
naranja=#EDB888
letras_negras=#8C8B8B


@font-face
	font-family Pacifico
	src url(fuentes/Pacifico.ttf) format('truetype')

@font-face
	font-family Handlee-Regular
	src url(fuentes/Handlee-Regular.ttf) format('truetype')

@font-face
	font-family RockSalt
	src url(fuentes/RockSalt.ttf) format('truetype')

body
	text-align left
	margin 0px
	padding 0px
	background colorbody

	
header
	width 100%
	height auto
	margin 0px
	background red
	text-align left
	background #E5E5E5

	.caja_header
		width 65%
		padding 0px
		height auto
		margin auto
		margin-top 80px
		text-align left

		.logo
			float left
			width 100px
			height 100px
			transition transform 1s
			margin-top -20px
			
			img
				transition transform 1s

			img:hover
				transform scale(1.2,1.2)
						

		.titulos
			display inline-block
			margin 0px 0px 0px -10px	

			.nombre_empresa
				font-family Pacifico
				margin 0px
				padding -5px 8px -5px 08px
				font-size 40px
				color #33958F
				text-shadow: 2px 2px 5px rgba(173,213,170,0.5)

					
							
			.razon_social
				margin: -15px 0px 0px 25px
				padding:0px;
				font-size 23px
				color negro
				letter-spacing: 0px;
				text-shadow: 2px 2px 5px rgba(0,0,0,0.3);
				font-family Handlee-Regular
				font-weight: bold

					
		nav
			display inline-block
			float right
			margin 12px 0px 0px 0px
			padding 0px
						
			ul
				text-align center
				padding 0px
								
				li
					display inline
					font-size 25px
					margin-right 10px
					margin-left 10px
									
					a
						text-decoration none
						color negro
						transition transform 1s
						font-family Handlee-Regular
						font-weight: bold
							
					.nav_index
						color #33958F

					a:hover
						color #33958F
						transform scale(1.2,1.2)


section
	
.index
	width 80%
	height auto
	margin auto
	position relative
	
	.contenido
		display inline-block
		position relative
		margin 100px 0px 100px 0px
		padding 0px
		overflow hidden

		.logobody
			float left
			width 450px
			height 450px
			margin 0px 0px 0px 0px
			padding 0px

		.saludos
			float left
			margin 0px 0px 0px 0px
			padding 0px 0px 0px 30px
			
			.hola
				display block
				font-size 80px
				font-weight bold
				color letras_negras
			.sabemos
				display block
				font-size 50px
				font-weight bold
				color letras_negras

			.logos_paginas_index
				padding 30px 0px 30px 0px
				margin 0px
			.descripcion
				display block
				font-size 30px
				font-weight bold
				color letras_negras
			.movil_web
				font-size 30px
				font-weight bold
				color letras_negras
			.en_una
				font-size 30px
				font-weight bold
				color #E57D22
	
.servicios
	display block
	margin 0px 
	padding 0px
	width 80%
	height auto
	margin auto

	.titulo_servicios
		width 100%
		height auto
		margin 0px
		padding 0px
		display block
		background white

		h1
			color colortitulos
			display inline-block
			background white
			font-size 80px
			width 90%
			height auto
			margin 50px 20px 20px 20px 
			padding 0px 0px 2% 0px
	

article
	margin 20px 0px 20px 0px
	padding 0px
	display inline-block
	background white
	border-top 3px solid naranja
		
	figure
		margin 0px
		padding 0px
		background white

		img
			margin 0px
			padding 20px
			float left

	div
		margin 0px
		padding 7% 7% 0px 0px
		background white

		p
			margin 0px
			padding 0px

	

.informacion
